{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "DatabaseType": "NpgSql",
  "ConnectionStrings": {
    "NpgSql": "Server=localhost;Database=MyDatabase;User Id=myuser;Password=mypassword;",    
    "MsSql": "Server=localhost;Database=MyDatabase;User Id=myuser;Password=mypassword;"
  },
  "DatabaseOptions": {
    "CommandTimeour": 30,
    "MaxRetryCount": 3,
    "EnableSensitiveDataLogging": true,
    "MaxRetryDelay": 3,
    "EnableDetailedErrors": true
  }
}